{

	"manifest_version" : 2,

	"name" : "Close left tabs (white icon)",
	"version" : "0.3.1",
	"description" : "Close Left Tabs on one click",

	"permissions" : [
		"contextMenus"
	],

	"browser_action" : {
		"default_icon" : "icons-white/icon.png",
		"default_title" : "Close Left Tabs"
	},

	"background" : {

		"scripts" : ["background.js"]

	},

	"icons": { 
			"16": "icons-white/icon16.png",
           "48": "icons-white/icon48.png",
          "128": "icons-white/icon128.png" }

}